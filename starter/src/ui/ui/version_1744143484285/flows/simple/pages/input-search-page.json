{
  "title": "",
  "description": "",
  "variables": {
    "backgroundColor": {
      "type": "string",
      "defaultValue": "dark-ocean"
    },
    "illustrationBackground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png"
    },
    "illustrationForeground": {
      "type": "string",
      "defaultValue": "resources/images/AIflower.png"
    },
    "imageStretch": {
      "type": "string",
      "defaultValue": "none"
    },
    "mobileImage": {
      "type": "string",
      "defaultValue": "on"
    },
    "bucketName": {
      "type": "string"
    },
    "hitTotal": {
      "type": "number",
      "defaultValue": 0
    },
    "namespace": {
      "type": "string",
      "input": "fromUrl"
    },
    "originButton": {
      "type": "string"
    },
    "preAuthRequestURL": {
      "type": "string"
    },
    "queryResponse": {
      "type": "queryResponseType"
    },
    "ragDocument": {
      "type": "string"
    },
    "ragResponse": {
      "type": "string"
    },
    "rawSearch": {
      "type": "string"
    },
    "searchHit": {
      "type": "queryHit[]"
    },
    "searchHitADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.searchHit }}",
        "itemType": "queryHit"
      }
    },
    "searchText": {
      "type": "string"
    },
    "testListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "keyAttributes": "@value",
        "itemsPath": "items",
        "responseType": "getallTestResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "title"
          ]
        },
        "fetchChainId": "getall_TestFetchAction"
      }
    }

  },
  "metadata": {},
  "types": {
    "@decorators.getallTestResponse": {
      "endpoint": "getall_Test",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.queryResponseType": {
      "endpoint": "postQuery",
      "service": "query",
      "path": "response"
    },
    "@decorators.searchResponse": {
      "endpoint": "search",
      "service": "opensearch",
      "path": "response"
    },
    "getallTestResponse": {
      "items": [
        {
          "title": "string"
        }
      ]
    },
    "queryHit": {
      "content": "string",
      "contentType": "string",
      "filename": "string",
      "page": "number",
      "path": "string",
      "region": "string",
      "summary": "string"
    },
    "queryResponseType": [
      {
        "content": "string",
        "contentType": "string",
        "filename": "string",
        "page": "number",
        "path": "string",
        "region": "string",
        "summary": "string"
      }
    ]

  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "originButton": "semantic"
          }
        }
      ]
    },
    "buttonAction2": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "originButton": "rag"
          }
        }
      ]
    },
    "buttonAction3": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "originButton": "generate"
          }
        }
      ]
    },
    "buttonAction4": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "originButton": "hybrid"
          }
        }
      ]
    },
    "buttonAction5": {
      "chains": [
        {
          "chain": "callDocZoom",
          "parameters": {
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "buttonAction6": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "originButton": "langchain"
          }
        }
      ]
    },
    "inputSearchValueAction": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "itemContext": "{{$event.detail.itemContext}}",
            "value": "{{$event.detail.value}}",
            "originButton": "search"
          }
        }
      ]
    },
    "searchAction": {
      "chains": [
        {
          "chain": "inputSearch",
          "parameters": {
            "originButton": "search"
          }
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-input-search": {
        "path": "ojs/ojinputsearch"
      },
      "oj-list-item-layout": {
        "path": "ojs/ojlistitemlayout"
      },
      "oj-list-view": {
        "path": "ojs/ojlistview"
      },
      "oj-text-area": {
        "path": "ojs/ojinputtext"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-sp-welcome-page": {
        "path": "oj-sp/welcome-page/loader"
      }

    }
  },
  "events": {},
  "constants": {}
}