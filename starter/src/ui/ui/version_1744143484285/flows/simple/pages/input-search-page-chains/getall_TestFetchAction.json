{
  "root": "callRestEndpoint1",
  "actions": {
    "callRestEndpoint1": {
      "module": "vb/action/builtin/restAction",
      "parameters": {
        "requestType": "json",
        "endpoint": "businessObjects/getall_Test",
        "responseType": "getallTestResponse",
        "hookHandler": "{{ $variables.configuration.hookHandler }}",
        "uriParams": {
          "orderBy": "title"
        }
      },
      "outcomes": {
        "success": "return1"
      }
    },
    "return1": {
      "module": "vb/action/builtin/returnAction",
      "parameters": {
        "outcome": "success",
        "payload": "{{ $chain.results.callRestEndpoint1 }}"
      }
    }
  },
  "variables": {
    "configuration": {
      "type": {
        "hookHandler": "vb/RestHookHandler"
      },
      "input": "fromCaller",
      "required": true
    }
  }
}